{
  "name": "djifpvoutstream",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc && yarn minify",
    "dev": "yarn ts-node ./src/",
    "prod": "yarn build && node ./dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "exe": "yarn build && yarn pkg ./dist/index.js --config package.json",
    "minify": "yarn uglifyjs --compress --mangle -- ./dist/index.js -o ./dist/index.js &&  yarn uglifyjs --compress --mangle -- ./dist/goggles.js -o ./dist/goggles.js"
  },
  "author": "Kacper Olszanski",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^16.14.2",
    "@types/yargs": "^17.0.9",
    "usb": "^2.1.2",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "pkg": "^5.6.0",
    "prebuildify": "^5.0.0",
    "prebuildify-ci": "^1.0.5",
    "ts-node": "^10.6.0",
    "typescript": "^4.6.2",
    "uglify-js": "^3.15.4"
  },
  "pkg": {
    "scripts": "./dist/*/*.js",
    "outputPath": "./bin",
    "assets": [
      "./stuffneeded/**",
      "./node_modules/usb/**"
    ],
    "targets": [
      "node16-win-x64",
      "node16-linux-x64",
      "node16-macos-x64"
    ]
  }
}
